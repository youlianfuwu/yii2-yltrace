{
    "name": "youlianfuwu/yii2-yltrace",
    "description": "Yii2 extension, used to trace metrics with OpenTelemetry",
    "type": "yii2-extension",
    "version": "dev-master",
    "keywords": ["yii2","trace","OpenTelemetry", "youlianfuwu"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "ellios",
            "email": "wangranyang@youlianfuwu.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.39",
        "open-telemetry/opentelemetry": "^1.0",
        "guzzlehttp/promises": "^2.0",
        "php-http/message-factory": "^1.1",
        "php-http/httplug": "^2.4",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "yiisoft/yii2-dev": "~2.0.39"
    },
    "autoload": {
        "psr-4": {"yii\\yltrace\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "yii\\yltrace\\": "tests/"}
    },
    "extra": {
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "path",
            "url": "/opt/youliancode/yii2-yltrace"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "php-http/discovery": false
        }
    }
}
